{
  "name": "pug-php/pug-symfony",
  "replace": {
    "kylekatarnls/jade-symfony": "self.version"
  },
  "description": "Pug template engine for Symfony",
  "type": "library",
  "require": {
    "php": ">=5.4.0",
    "pug/installer": "^0.1.0",
    "pug-php/pug": "^2.1.3",
    "pug-php/pug-assets": "^1.0.1",
    "symfony/symfony": "4.0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.7 || ~6.3.0",
    "kylekatarnls-codeclimate/php-test-reporter": "dev-symfony-4",
    "satooshi/php-coveralls": "dev-symfony-4",
    "composer/composer": "dev-symfony-4"
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/kylekatarnls/composer.git"
    },
    {
      "type": "git",
      "url": "https://github.com/kylekatarnls/php-test-reporter.git"
    },
    {
      "type": "git",
      "url": "https://github.com/kylekatarnls/php-coveralls.git"
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kyle Katarn",
      "email": "kylekatarnls@gmail.com"
    }
  ],
  "autoload": {
    "psr-0": {
      "Jade": "src/",
      "Pug": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Pug\\Installer\\Installer::onAutoloadDump"
    ],
    "post-update-cmd": [
      "Pug\\Installer\\Installer::onAutoloadDump"
    ]
  },
  "extra": {
    "installer": "Pug\\PugSymfonyEngine::install"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    }
  }
}
